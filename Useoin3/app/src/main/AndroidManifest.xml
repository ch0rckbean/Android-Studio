<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.useoin3">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/moon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Forest" android:label="forest"/>
        <activity android:name=".Bug" android:label="bug"/>
        <activity android:name=".Wave" android:label="wave"/>
        <activity android:name=".Rain" android:label="rain"/>
        <activity android:name=".MyPage" android:label="mypage"/>
        <activity android:name=".WhiteNoise" android:label="whitenoise"/>
        <activity android:name=".SecondPage" android:label="secondpage"/>
    </application>

</manifest>